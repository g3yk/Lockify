    <!-- Form section -->
    <section class="u-clearfix u-section-1" id="sec-b0a6">
        <div class="u-clearfix u-sheet u-sheet-1">
            <div class="u-form u-form-1">
                <!-- Form for user input -->
                <form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" source="email" name="form" style="padding: 10px;">
                    <div class="u-form-email u-form-group">
                        <label for="email-96b3" class="u-label">Email</label>
                        <input type="email" placeholder="Geçerli bir e-posta adresi girin" id="email-96b3" name="email" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-form-group u-form-name u-form-group-2">
                        <label for="name-d06d" class="u-label">Username</label>
                        <input type="text" placeholder="Username giriniz" id="name-d06d" name="username" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-form-group u-form-message">
                        <label for="message-96b3" class="u-label">Biyografi</label>
                        <textarea placeholder="Biyografi giriniz" rows="4" cols="50" id="message-96b3" name="message" class="u-input u-input-rectangle" required=""></textarea>
                    </div>
                    <div class="u-form-group u-form-name u-form-partition-factor-2 u-form-group-4">
                        <label for="name-3892" class="u-label">Adı</label>
                        <input type="text" placeholder="Adınızı Girin" id="name-3892" name="name-1" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-form-group u-form-name u-form-partition-factor-2 u-form-group-5">
                        <label for="name-3892" class="u-label">Soyadı</label>
                        <input type="text" placeholder="Soyadınızı Girin" id="name-3892" name="name-2" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-form-date u-form-group u-form-group-6">
                        <label for="date-a723" class="u-label">Doğum Tarihiniz</label>
                        <input type="text" placeholder="AY/GÜN/YIL" id="date-a723" name="date" class="u-input u-input-rectangle" required="" data-date-format="mm/dd/yyyy">
                    </div>
                    <div class="u-form-group u-form-phone u-form-group-7">
                        <label for="phone-6325" class="u-label">Telefon</label>
                        <input type="tel" pattern="\+?\d{0,3}[\s\(\-]?([0-9]{2,3})[\s\)\-]?([\s\-]?)([0-9]{3})[\s\-]?([0-9]{2})[\s\-]?([0-9]{2})" placeholder="Telefonunuzu girin (örn. +14155552675)" id="phone-6325" name="phone" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-form-country u-form-group u-form-group-8">
                        <label for="country-690e" class="u-label">Ülke</label>
                        <div class="u-form-select-wrapper">
                            <select id="country-690e" name="country" class="u-input u-input-rectangle">
                                <option value="United States">United States</option>
                            </select>
                        </div>
                    </div>
                    <div class="u-form-email u-form-group u-form-group-9">
                        <label for="email-f669" class="u-label">Şehir</label>
                        <input type="email" placeholder="Şehir" id="email-f669" name="city" class="u-input u-input-rectangle" required="">
                    </div>
                    <div class="u-align-center u-form-group u-form-submit">
                        <a href="#" class="u-btn u-btn-submit u-button-style">Kaydet</a>
                        <input type="submit" value="submit" class="u-form-control-hidden">
                    </div>
                    <!-- Success and error messages after form submission -->
                    <div class="u-form-send-message u-form-send-success"> Teşekkür ederiz! Mesajınız gönderildi. </div>
                    <div class="u-form-send-error u-form-send-message"> Mesajınız gönderilemedi. Lütfen hataları düzeltin ve tekrar deneyin. </div>
                    <input type="hidden" value="" name="recaptchaResponse">
                    <input type="hidden" name="formServices" value="656c9eb2-c226-f983-15fe-766e595c2213">
